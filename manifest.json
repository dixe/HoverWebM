{
  "manifest_version": 2,

  "name": "WebM Hover",
  "description": "This extensions is like hover zoom, but for WebM",
  "version": "1.0",

  "permissions": [
  "tabs","<all_urls>"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "css":["mycss.css"],
    "js": ["jquery.js","contentscript.js"]
  }]
}